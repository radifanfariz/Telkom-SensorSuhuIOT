<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mediumsitompul.sensortemperatureiot">

    <application
        android:name=".FireApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_telkom"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_telkom_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SensorTemperatureIOT"
        android:usesCleartextTraffic="true">
        <activity android:name=".MainActivityTable"></activity>
        <activity android:name=".FirstActivity" />
        <activity android:name=".Login2"
            android:theme="@style/Theme.SensorTemperatureIOT">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>